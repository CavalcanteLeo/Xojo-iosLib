#tag Module
Protected Module BooleanExtension
	#tag Method, Flags = &h0
		Function toText(extends b as Boolean) As Text
		  Return if (b, BoolTrue, BoolFalse)
		End Function
	#tag EndMethod


	#tag Constant, Name = BoolFalse, Type = Text, Dynamic = False, Default = \"False", Scope = Private
	#tag EndConstant

	#tag Constant, Name = BoolTrue, Type = Text, Dynamic = False, Default = \"True", Scope = Private
	#tag EndConstant


	#tag ViewBehavior
		#tag ViewProperty
			Name="Index"
			Visible=true
			Group="ID"
			InitialValue="-2147483648"
			Type="Integer"
		#tag EndViewProperty
		#tag ViewProperty
			Name="Left"
			Visible=true
			Group="Position"
			InitialValue="0"
			Type="Integer"
		#tag EndViewProperty
		#tag ViewProperty
			Name="Name"
			Visible=true
			Group="ID"
			Type="String"
		#tag EndViewProperty
		#tag ViewProperty
			Name="Super"
			Visible=true
			Group="ID"
			Type="String"
		#tag EndViewProperty
		#tag ViewProperty
			Name="Top"
			Visible=true
			Group="Position"
			InitialValue="0"
			Type="Integer"
		#tag EndViewProperty
	#tag EndViewBehavior
End Module
#tag EndModule
