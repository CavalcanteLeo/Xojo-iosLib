#tag Events HTMLViewer1
	#tag Event
		Sub Open()
		  me.addcolumn (Array("Apple","Banana", "Pear", "Orange", "Pineapple", "Grapefruit", "Cherry", "Strawberry"))
		  me.AddColumn(Array("Punch","Soda"))
		  me.SelectedRow(0) = 1
		  me.SelectedRow(1) = 1
		  dim mysize as nssize = me.iOSLibView.SizeThatFits
		  me.dataSource.setRowHeight (0,26)
		  
		End Sub
	#tag EndEvent
	#tag Event
		Sub SelectionChanged(row as integer, column as integer)
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Canvas1
	#tag Event
		Sub Open()
		  dim colorarray as new iOSLibMutableArray (2)
		  colorarray.Addobject new iOSLibCGColor(&c11205400)
		  colorarray.Addobject new iOSLibCGColor( &c0D112700)
		  me.Layer.Colors = colorarray
		  me.Layer.ContentsPositioning = iOSLibCALayer.CaLayerContentPosition.BottomRight
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Button1
	#tag Event
		Sub Action()
		  // dim mylayer as new iOSLibCAGradientLayer ()
		  // mylayer.Bounds = canvas1.CoreAnimationLayer.Frame
		  // canvas1.CoreAnimationLayer.AddSubLayer mylayer
		  
		  animationcanvas.CoreAnimationLayer.ContentsScale = 4
		  
		  me.CoreAnimationLayer.BackgroundColor = new iOSLibcgColor (&c394D7E00)
		  me.CoreAnimationLayer.CornerRadius = 8
		  dim myvalues as new iOSLibMutableArray (5)
		  dim mypos as Double
		  mypos= 10
		  myvalues.Addobject new iOSLibNumber(mypos)
		  mypos = -10
		  myvalues.Addobject new iOSLibNumber(mypos)
		  mypos=5
		  myvalues.Addobject new iOSLibNumber(mypos)
		  mypos = -5
		  myvalues.Addobject new iOSLibNumber(mypos)
		  mypos = 0
		  myvalues.Addobject new iOSLibNumber(mypos)
		  
		  dim myanimation as new iOSLibCAKeyframeAnimation (iOSLIbCABasicAnimation.Properties.height, myvalues)
		  myanimation.Duration = 0.5
		  myanimation.Additive = true
		  me.CoreAnimationLayer.AddAnimation myanimation
		  //
		  
		  
		  dim myanimation1 as new iOSLibCAKeyframeAnimation (iOSLibCAAnimation.Properties.Position)
		  dim newrect as new rect (25,25, Canvas1.height-25,Canvas1.Height-25)
		  myanimation1.Path = new iOSLibCGPathEllipse (newrect.tonsrect)
		  myanimation1.Duration = 4
		  myanimation.Additive = true
		  myanimation1.RepeatCount = 2
		  myanimation1.RotationMode = iOSLibCAKeyframeAnimation.RotationModes.RotateAuto
		  myanimation1.CalculationMode = iOSLibCAKeyframeAnimation.ValueCalculationModes.Paced
		  animationcanvas.CoreAnimationLayer.AddAnimation myanimation1
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events animationcanvas
	#tag Event
		Sub Open()
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Button2
	#tag Event
		Sub Action()
		  for q as integer = 0 to 10000
		    dim mycolor as new iOSLIbCABasicAnimation (iOSLIbCABasicAnimation.Properties.BackgroundColor,  canvas1.CoreAnimationLayer.BackgroundColor, new iOSLibCGColor (&c11223344))
		    dim newframe as new rect (10,10,80,80)
		    dim myanimation as new iOSLIbCABasicAnimation (iOSLIbCABasicAnimation.Properties.Bounds,  canvas1.CoreAnimationLayer.Bounds, newframe.tonsrect)
		    
		    dim mygroup as new iOSLibCAAnimationGroup
		    dim myarray as new iOSLibMutableArray(2)
		    myarray.Addobject mycolor
		    myarray.Addobject myanimation
		    mygroup.Animations = myarray
		    
		    me.CoreAnimationLayer.AddAnimation mygroup
		  next
		End Sub
	#tag EndEvent
#tag EndEvents
