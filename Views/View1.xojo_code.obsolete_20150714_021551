#tag WindowCode
	#tag Property, Flags = &h0
		SCV As ScaleView
	#tag EndProperty


#tag EndWindowCode

#tag Events AppleViewControl1
	#tag Event
		Sub Open()
		  dim picview as new AppleImageView(new appleimage(iosLibLogo))
		  me.AppleView.ContentView = picview
		  me.BackgroundColor = &cffffff00
		  
		End Sub
	#tag EndEvent
#tag EndEvents
#tag Events Button1
	#tag Event
		Sub Action()
		  dim cc as new ScaleView
		  // cc.AppleView.frame.Size_.width = AppleViewControl1.Width
		  // cc.AppleView.frame.Size_.Height = AppleViewControl1.Height
		  cc.View.bounds.Size_.height = 4000
		  cc.View.frame.Size_.height = 4000
		  
		  AppleViewControl1.AppleView.ContentView = new appleview(cc.ViewHandle)
		  scv= cc
		  AppleViewControl1.AppleView.ContentSize = nssize (800, 4000)
		  System.debuglog AppleViewControl1.DecelerationRate.totext
		End Sub
	#tag EndEvent
#tag EndEvents
