		Sub ValueChanged()
		  select case me.Value
		  case 0, 2
		    Slider1.MinValue = -360
		    slider1.MaxValue = 360
		    Slider1.value = 0
		  case 1
		    slider1.MinValue = 0
		    slider1.MaxValue = 20
		    slider1.value = 1
		  end select
		End Sub
	#tag EndEvent
