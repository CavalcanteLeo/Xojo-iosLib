		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Attributes( hidden )  Sub informOnURLSessiondidReceiveChallengecompletionHandler(challenge as ptr)
		  RaiseEvent URLSessiondidReceiveChallenge (new AppleURLAuthenticationChallenge(challenge))
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Attributes( hidden )  Sub informOnURLSessiontaskdidCompleteWithError(task as ptr, error as ptr)
		  RaiseEvent URLSessiontaskdidCompleteWithError (new AppleURLSessiont(challenge))
		End Sub
	#tag EndMethod


	#tag Hook, Flags = &h0
