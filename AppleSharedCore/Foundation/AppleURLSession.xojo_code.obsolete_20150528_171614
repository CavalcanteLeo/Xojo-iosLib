		  RetainClassObject
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Attributes( hidden )  Sub informonURLSessiondidBecomeInvalidWithError(Error as Ptr)
		  RaiseEvent URLSessiondidBecomeInvalidWithError (new AppleError(error))
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Attributes( hidden )  Sub informonURLSessionDidFinishEventsForBackgroundURLSession()
