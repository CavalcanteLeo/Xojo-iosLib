		  Super.Constructor (sharedSession(ClassPtr))
		  RetainClassObject
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h1000
		Sub constructor(Configuration as AppleURLSessionConfiguration)
		  Declare Function sessionWithConfiguration lib Foundation selector "sessionWithConfiguration:" (id as ptr, configuration as ptr) as ptr
		  
