#tag Class
Protected Class iOSLIbCanvas
Inherits iOSLibResponder
	#tag Event
		Sub Close()
		  id.RemoveControl
		  me.id = nil
		End Sub
	#tag EndEvent

	#tag Event
		Function CreateView() As UInteger
		  dim myview as new appleview (FoundationFrameWork.NSMakeRect(0,0,100,100))
		  id = myview
		  myview.RegisterControl (self)
		  myview.BackgroundColor = AppleColor.WhiteColor
		  
		  return uinteger(myview.id)
		End Function
	#tag EndEvent


	#tag Method, Flags = &h0
		Attributes( hidden )  Sub informonAnimationFinished(completed as Boolean)
		  RaiseEvent AnimationFinished(completed)
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
		Attributes( hiddden )  Sub informonDidAddSubview(View as appleview)
		  RaiseEvent AddedSubView (view)
		  
		  
		End Sub
	#tag EndMethod

	#tag Method, Flags = &h0
