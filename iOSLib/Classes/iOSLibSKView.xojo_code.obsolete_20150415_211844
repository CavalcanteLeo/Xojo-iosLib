			  Return showsFPS (id)
			End Get
		#tag EndGetter
		#tag Setter
			Set
			  Declare Sub setShowsFPS lib SpriteKit selector "setShowsFPS:" (id as ptr, value as Boolean)
			  setShowsFPS id, value
			End Set
		#tag EndSetter
